# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=il-central-1

# S3 Configuration
S3_BUCKET_NAME=ais-research-data-archive
S3_ENDPOINT_URL=https://s3.amazonaws.com

# Performance Optimization (for large-scale uploads)
MAX_CONCURRENT_PARTS=20          # Number of concurrent parts in multipart upload
PART_SIZE_MB=200                 # Part size in MB for multipart uploads
MAX_RETRIES=5                    # Maximum retry attempts for failed operations
MAX_CONCURRENT_FILES=5           # Number of files to upload concurrently

# AIS Data Configuration
AIS_BASE_PATH=E:\AISDData\exactEarth
SCAN_FILE_TYPES=.rar,.zip,.tar.gz
LOG_LEVEL=INFO
CHUNK_SIZE_MB=100

# File Processing Configuration
SKIP_EXISTING_FILES=true
VALIDATE_CHECKSUMS=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=ais_upload.log

# Progress Tracking
SAVE_PROGRESS_INTERVAL=10
SHOW_PROGRESS_BAR=true
VERBOSE_OUTPUT=true

# Performance Optimization
BUFFER_SIZE=8192
MAX_MEMORY_USAGE_MB=512

# Error Handling
CONTINUE_ON_ERROR=true
RETRY_FAILED_FILES=true
MAX_RETRY_ATTEMPTS=3
